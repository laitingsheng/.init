  "wsl": {
    "automount": {
      "enabled": true,
      "root": "/mnt/",
      "options": "metadata,umask=0022,fmask=0133,dmask=0022",
      "mountFsTab": true
    },
    "network": {
      "generateHosts": true,
      "generateResolvConf": true
    },
    "interop": {
      "enabled": true,
      "appendWindowsPath": false
    },
    "user": {
      "default": null
    },
    "wsl2": {
      "guiapplications": true
    }
  }
}
