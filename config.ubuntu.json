{
  "variables": {
    "desktop": false,
    "wsl": true,
    "keep_old_config": false,
    "autoremove": false,
    "upgrade": true,
    "max_download_size": null,
    "max_install_size": null,
    "release": "22.04",
    "codename": "jammy"
  },
  "apt": {
    "repos": {
      "ubuntu": {
        "uri": "https://mirrors.aliyun.com/ubuntu",
        "codename_suffices": [
          "",
          "-updates",
          "-security",
          "-proposed",
          "-backports"
        ],
        "components": [
          "main",
          "restricted",
          "universe",
          "multiverse"
        ]
      },
      "ubuntu.ports": {
        "uri": "https://mirrors.aliyun.com/ubuntu-ports",
        "architectures": [
          "armhf",
          "arm64"
        ],
        "codename_suffices": [
          "",
          "-updates",
          "-security",
          "-proposed",
          "-backports"
        ],
        "components": [
          "main",
          "restricted",
          "universe",
          "multiverse"
        ]
      },
      "dotnet": {
        "uri": {
          "template": "https://packages.microsoft.com/ubuntu/{release}/prod",
          "format_args": {
            "release": {
              "type": "variable",
              "name": "release"
            }
          }
        }
      },
      "githubcli": {
        "uri": "https://cli.github.com/packages",
        "dists": [
          "stable"
        ],
        "components": [
          "main"
        ]
      },
      "node": {
        "uri": {
          "template": "https://deb.nodesource.com/node_{version}.x",
          "format_args": {
            "version": 16
          }
        }
      },
      "docker": {
        "uri": "https://download.docker.com/linux/ubuntu",
        "components": [
          "stable"
        ]
      },
      "kubernetes": {
        "uri": "https://mirrors.aliyun.com/kubernetes/apt",
        "dists": [
          "kubernetes-xenial"
        ]
      },
      "kitware": {
        "uri": "https://apt.kitware.com/ubuntu"
      },
      "redis": {
        "uri": "https://packages.redis.io/deb"
      },
      "nginx": {
        "uri": "https://nginx.org/packages/ubuntu",
        "components": [
          "nginx"
        ]
      },
      "cuda": {
        "uri": {
          "template": "https://developer.download.nvidia.cn/compute/cuda/repos/{folder}/x86_64",
          "format_args": {
            "folder": {
              "type": "method",
              "name": "cuda_uri_folder"
            }
          }
        },
        "dists": [
          "/"
        ],
        "components": []
      },
      "nvidia.container": {
        "url": {
          "template": "https://nvidia.github.io/libnvidia-container/ubuntu{release}/libnvidia-container.list",
          "format_args": {
            "release": {
              "type": "variable",
              "name": "release"
            }
          }
        }
      },
      "apache.couchdb": {
        "uri": "https://apache.jfrog.io/artifactory/couchdb-deb"
      },
      "mongodb": {
        "uri": "https://repo.mongodb.org/apt/ubuntu",
        "dists": [
          "focal/mongodb-org/5.0"
        ],
        "components": [
          "multiverse"
        ]
      }
    },
    "keys": {
      "ubuntu": "file:///usr/share/keyrings/ubuntu-archive-keyring.gpg",
      "microsoft": "https://packages.microsoft.com/keys/microsoft.asc",
      "githubcli": "https://cli.github.com/packages/githubcli-archive-keyring.gpg",
      "node": "https://deb.nodesource.com/gpgkey/nodesource.gpg.key",
      "docker": "https://download.docker.com/linux/ubuntu/gpg",
      "kubernetes": "https://mirrors.aliyun.com/kubernetes/apt/doc/apt-key.gpg",
      "kitware": "https://apt.kitware.com/keys/kitware-archive-latest.asc",
      "redis": "https://packages.redis.io/gpg",
      "nginx": "https://nginx.org/keys/nginx_signing.key",
      "cuda": {
        "template": "https://developer.download.nvidia.cn/compute/cuda/repos/{folder}/x86_64/3bf863cc.pub",
        "format_args": {
          "folder": {
            "type": "method",
            "name": "cuda_uri_folder"
          }
        }
      },
      "nvidia.container": "https://nvidia.github.io/libnvidia-container/gpgkey",
      "apache.couchdb": "https://couchdb.apache.org/repo/keys.asc",
      "mongodb": "https://www.mongodb.org/static/pgp/server-5.0.asc"
    },
    "packages": [
      "ubuntu-minimal",
